    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.0",
    "chai": "^3.5.0",
    "cors": "^2.8.1",
    "express": "^4.14.1",
    "faker": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
