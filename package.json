    "eslint-plugin-react": "^6.9.0",
    "express": "^4.14.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.2.0",
    "sequelize-cli": "^2.5.1",
    "supertest": "^3.0.0",
    "webpack": "^1.14.0"
  },
