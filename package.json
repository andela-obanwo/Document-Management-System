{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "A document management system offering roles and access restrictions",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./server/tests,./coverage --copy-files",
    "test": "npm run cover && npm run coveralls",
    "cover": "NODE_ENV=test istanbul cover _mocha -- server/tests/**/*.js --compilers js:babel-core/register --timeout 20000",
    "start-old": "nodemon ./server/Server.js --exec babel-node --presets es2015",
    "start-server": "node ./dist/server/Server.js",
    "start": "npm run build && npm run start-server",
    "test-seed": "sequelize db:seed --seed 20170221112635-AccessTypes.js --env 'test'",
    "test-migrate": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-obanwo/Document-Management-System.git"
  },
  "author": "Oreoluwa Banwo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-obanwo/Document-Management-System/issues"
  },
  "homepage": "https://github.com/andela-obanwo/Document-Management-System#readme",
  "devDependencies": {
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.14.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.2.0",
    "sequelize-cli": "^2.5.1",
    "supertest": "^3.0.0",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.21.2",
    "babel-core": "^6.21.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.0",
    "chai": "^3.5.0",
    "cors": "^2.8.1",
    "coveralls": "^2.11.16",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "faker": "^4.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "jsonwebtoken": "^7.2.1",
    "mocha": "^3.2.0",
    "morgan": "^1.8.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.1",
    "sequelize-cli": "^2.5.1",
    "supertest": "^3.0.0",
    "webpack": "^1.14.0"
  }
}
